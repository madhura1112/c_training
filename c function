// write a c function which will accept an array arr its size an n,returns the differnce between the first and last element
#include <stdio.h>
#include <stdio.h>

int sub_arr(int arr [],int n)
{
    int sub = arr[0]-arr[n-1];
    return abs (sub);
}
void main()
{


int arr[3]={54,76,89};
int n=3
printf("%d",sub_arr(arr,n));
}